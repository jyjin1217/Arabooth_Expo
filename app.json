{
  "expo": {
    "name": "AraBooth",
    "slug": "AraBooth",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/arabooth_1024.png",
    "splash": {
      "image": "./assets/ara_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mocha.Arabooth"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ara_foreground.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.mocha.Arabooth"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
