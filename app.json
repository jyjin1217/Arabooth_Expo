{
  "expo": {
    "name": "AraBooth",
    "slug": "AraBooth",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/araIcon.png",
    "splash": {
      "image": "./assets/araSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mocha.Arabooth"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/araIcon.png",
        "backgroundColor": "#00c4cc"
      },
      "package": "com.mocha.Arabooth"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
