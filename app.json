{
  "expo": {
    "name": "AraBooth",
    "slug": "AraBooth",
    "version": "1.0.9",
    "orientation": "portrait",
    "icon": "./assets/araIcon.png",
    "splash": {
      "image": "./assets/araSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.worknall.arabooth",
      "config":{
        "usesNonExemptEncryption":false
      },
      "infoPlist":{
        "NSCameraUsageDescription":"This app uses the camera to scan QR code on arabooth"
      }
    },
    "android": {
      "versionCode":4,
      "package": "com.mocha.Arabooth"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
